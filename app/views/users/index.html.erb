<div class="projects-container">
<h1>USERS</h1>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>User Id</th>
      <th>User Email</th>
      <th>Joined On</th>
    </tr>
  </thead>

  <tbody>
    
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= user.created_at %></td><td>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</div>